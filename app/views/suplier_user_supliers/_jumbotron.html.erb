<% suplier = current_user_suplier.suplier %>
<div class="jumbotron banner2" style="background-image: radial-gradient(rgba(255,255,255,0.5), rgba(0,0,0,0.5)), url(<%= render inline: suplier.valid_img_bg %>);">
  <h1 class="my-3 text-center"><%= suplier.name %></h1>
  <div class="row justify-content-end">
    <%= link_to "#edit suplier", edit_suplier_path(@suplier) %>
  </div>
</div>
